{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "root": "demo",
      "sourceRoot": "demo/src",
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app",
            "index": "demo/src/index.html",
            "main": "demo/src/main.ts",
            "polyfills": "demo/src/polyfills.ts",
            "tsConfig": "demo/tsconfig.app.json",
            "assets": [
              "demo/src/favicon.ico",
              "demo/src/assets"
            ],
            "styles": [
              "demo/src/styles.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "stylePreprocessorOptions": {
              "includePaths": [
                "./node_modules"
              ]
            },
            "allowedCommonJsDependencies": [
              "highlight-loader",
              "json-formatter-js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "demo/src/environments/environment.ts",
                  "with": "demo/src/environments/environment.prod.ts"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "800kb"
                }
              ]
            },
            "docker": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "demo/src/environments/environment.ts",
                  "with": "demo/src/environments/environment.docker.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "app:build:production"
            },
            "docker": {
              "buildTarget": "app:build:docker"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "app:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "demo/**/*.ts",
              "demo/**/*.html"
            ]
          }
        }
      }
    },
    "@rosoftlab/core": {
      "projectType": "library",
      "root": "projects/rosoftlab/core",
      "sourceRoot": "projects/rosoftlab/core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/rosoftlab/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/rosoftlab/core/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/rosoftlab/core/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/rosoftlab/core/src/test.ts",
            "tsConfig": "projects/rosoftlab/core/tsconfig.spec.json",
            "karmaConfig": "projects/rosoftlab/core/karma.conf.js"
          }
        }
      }
    },
    "@rosoftlab/ionic": {
      "projectType": "library",
      "root": "projects/rosoftlab/ionic",
      "sourceRoot": "projects/rosoftlab/ionic/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/rosoftlab/ionic/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/rosoftlab/ionic/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/rosoftlab/ionic/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/rosoftlab/ionic/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "@rosoftlab/formly": {
      "projectType": "library",
      "root": "projects/rosoftlab/formly",
      "sourceRoot": "projects/rosoftlab/formly/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/rosoftlab/formly/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/rosoftlab/formly/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/rosoftlab/formly/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/rosoftlab/formly/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "example": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/example",
      "sourceRoot": "projects/example/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/example",
            "index": "projects/example/src/index.html",
            "main": "projects/example/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/example/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/example/src/favicon.ico",
              "projects/example/src/assets"
            ],
            "styles": [
              "projects/example/src/global.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "docker": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "projects/example/src/environments/environment.ts",
                  "with": "projects/example/src/environments/environment.docker.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "example:build:production"
            },
            "development": {
              "buildTarget": "example:build:development"
            },
            "docker": {
              "buildTarget": "example:build:docker"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "example:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/example/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/example/src/favicon.ico",
              "projects/example/src/assets"
            ],
            "styles": [
              "projects/example/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "@rosoftlab/material": {
      "projectType": "library",
      "root": "projects/rosoftlab/material",
      "sourceRoot": "projects/rosoftlab/material/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/rosoftlab/material/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/rosoftlab/material/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/rosoftlab/material/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/rosoftlab/material/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "@rosoftlab/statemachine": {
      "projectType": "library",
      "root": "projects/rosoftlab/statemachine",
      "sourceRoot": "projects/rosoftlab/statemachine/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/rosoftlab/statemachine/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/rosoftlab/statemachine/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/rosoftlab/statemachine/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/rosoftlab/statemachine/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}