{
  "name": "@rosoftlab/core",
  "version": "1.0.5-alpha-10",
  "scripts": {
    "ng": "ng",
    "build": "ts-node --dir build build.ts",
    "builddev": "ts-node --dir build builddev.ts",
    "publish": "ts-node --dir build publish.ts",
    "unpublish": "ts-node --dir build unpublish.ts",
    "watch": "ng build @rosoftlab/rdict --watch --configuration development",
    "watch-core": "ng build @rosoftlab/core --watch --configuration development",
    "test": "ng test",
    "build-example": "ng build example",
    "start-docker": "ng serve example --configuration docker --port 19281 --host=localhost",
    "demo-docker": "npm run start-docker",
    "build-rdict": "ng build @rosoftlab/rdict --configuration development",
    "build-demo-rdict": "ng build rdict-example",
    "build-ionic-example": "ng build ionic-example-app",
    "build-fix-fuel": "ng build fix-fuel",
    "build-fix-fuel-prod": "ng build fix-fuel --configuration production",
    "demo-rdict": "ng serve rdict-example  --port 33080 --configuration development --hmr=false",
    "ionic-example": "ng serve ionic-example-app  --host 0.0.0.0  --port 19281 --configuration docker --hmr=false",
    "ionic-example-dev": "ng serve ionic-example-app  --host 0.0.0.0 --port 19281 --hmr=false",
    "fix-fuel": "ng serve fix-fuel  --host 0.0.0.0  --port 19281 --configuration docker --hmr=false",
    "fix-fuel-dev": "ng serve fix-fuel  --host 0.0.0.0 --port 19281 --hmr=false",
    "fix-fuel-dev-ssl": "ng serve fix-fuel  --host 0.0.0.0 --port 19281 --hmr=false --ssl true",
    "serve-fix-fuel-https": "npx http-server dist/fix-fuel/browser -p 19281 -c-1 --spa --ssl --cert certs/localhost.pem --key certs/localhost-key.pem -a 0.0.0.0",
    "pwa-fix-fuel-mobile": "npm run build-fix-fuel-prod && npm run serve-fix-fuel-https",
    "semantic-release": "semantic-release"
  },
