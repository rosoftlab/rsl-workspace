{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es2019",
    "lib": [
      "es2019",
      "dom"
    ],
    "outDir": "../../out-tsc/app",
    "types": [
      "@angular/localize"
    ],
    "paths": {
      "@rosoftlab/core": [
        "projects/rosoftlab/core/src/public-api.ts"
      ],
      "@rosoftlab/core/*": [
        "projects/rosoftlab/core/src/*"
      ],
      "@rosoftlab/rdict": [
        "projects/rosoftlab/rdict/src/public-api.ts"
      ],
      "@rosoftlab/rdict/*": [
        "projects/rosoftlab/rdict/src/*"
      ],
      "@rosoftlab/shared-ui": [
        "projects/rosoftlab/shared-ui/src/public-api.ts"
      ],
      "@rosoftlab/kendo": [
        "projects/rosoftlab/kendo/src/public-api.ts"
      ],
      "@rosoftlab/kendo/*": [
        "projects/rosoftlab/kendo/src/*"
      ]
    }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}